{
  "compilerOptions": {
    "lib": ["ESNext"],
    "target": "ESNext",
    "moduleResolution": "node",
    "module": "ESNext",
    "strict": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowJs": false,
    "checkJs": false,
    "maxNodeModuleJsDepth": 0,
    "forceConsistentCasingInFileNames": true,
    "experimentalDecorators":true
  },
  "files": [],
  "include": [],
  "references": [
    {
      "path": "./tsconfig.package.json"
    },
    {
      "path": "./tsconfig.test.json"
    }
  ]
}
